{
  "name": "mevauction-docs",
  "version": "0.0.12",
  "private": true,
  "description": "MEV Auction Documentation",
  "directories": {
    "doc": "docs",
    "public": "public"
  },
  "scripts": {
    "build": "mkdocs build -d public",
    "make": "bash build.sh",
    "fmt": "npx prettier --config .prettierrc.js --write .",
    "local-sitemap": "static-sitemap-cli --base http://localhost:3000 --no-clean --root public",
    "sitemap": "npx static-sitemap-cli --base https://mevauction.com --no-clean --root public"
  },
  "keywords": [
    "docs",
    "ethereum",
    "mev",
    "auction"
  ],
  "author": "SEE CONTRIBUTORS",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "markdownlint": "^0.33.0",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "static-sitemap-cli": "^2.2.3"
  }
}
